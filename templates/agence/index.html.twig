{% extends 'base.html.twig' %}

{% block title %}Agence Admin page{% endblock %}

{% block body %}
    <h1>Liste des Agences</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom_agence</th>
                <th>Email_agence</th>
                <th>Tel_agence</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for agence in agences %}
            <tr>
                <td>{{ agence.id }}</td>
                <td>{{ agence.nomAgence }}</td>
                <td>{{ agence.emailAgence }}</td>
                <td>{{ agence.telAgence }}</td>
                <td>
                    <a href="{{ path('app_agence_show', {'id': agence.id}) }}">show</a>
                    <a href="{{ path('app_agence_edit', {'id': agence.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_agence_new') }}">Create new</a>

            {# Add a button to go to index.html.twig #}
    <a href="{{ path('app_assurance_index') }}">Go to assurance</a>
{% endblock %}
