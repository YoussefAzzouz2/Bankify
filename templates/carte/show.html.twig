{% extends 'base.html.twig' %}

{% block title %}<title>Carte</title>{% endblock %}

{% block section %}
    <h1>Carte</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ carte.id }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ carte.NumC }}</td>
            </tr>
            <tr>
                <th>Date_Expiration</th>
                <td>{{ carte.DateExp ? carte.DateExp|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Network</th>
                <td>{{ carte.TypeC }}</td>
            </tr>
            <tr>
                <th>Statue</th>
                <td>{{ carte.StatutC }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_carte_index') }}">back to list</a>

    <a href="{{ path('app_carte_edit', {'id': carte.id}) }}">edit</a>

    {{ include('carte/_delete_form.html.twig') }}
{% endblock %}

